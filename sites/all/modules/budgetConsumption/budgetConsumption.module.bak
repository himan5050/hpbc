function budgetConsumption_access($op, $node, $account) {
	if($op == 'update' || $op == 'delete') {
		//&& ($account->uid == $node->uid)
		if (user_access('edit budgetConsumption', $account) ) {
			return TRUE;
		}
	}
	if (($op=='create') && ($op='list')) {
		return user_access('create budgetConsumption', $account);
	}
	if (($op=='view') or ($op=='list')) {
		return user_access('view budgetConsumption', $account);
	}
}
 
