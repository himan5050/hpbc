function budgetConsumption_load($node){
$sql = "SELECT * FROM node INNER JOIN tbl_budgetDistribution ON (node.nid=tbl_budgetDistribution.nid) WHERE node.nid='".$node->nid."'";
$res = db_query($sql);
return $rs = db_fetch_object($res);
}
  
